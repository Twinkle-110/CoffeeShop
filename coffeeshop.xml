  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
      <include name="forms_rt.jar"/>
    </fileset>
    <fileset dir="${idea.home}/plugins/java/lib">
      <include name="javac2.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>

  <!-- Modules -->

  <import file="${basedir}/.idea/module_coffeeshop.xml"/>

  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>

  <target name="clean" depends="clean.module.coffeeshop, clean.artifact.coffeeshop, clean.artifact.coffeeshop2" description="cleanup all"/>

  <target name="build.modules" depends="init, clean, compile.module.coffeeshop" description="build all modules"/>

  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.coffeeshop" value="${basedir}/out/artifacts/CoffeeShop"/>
    <property name="artifact.output.coffeeshop2" value="${basedir}/out/artifacts/CoffeeShop2"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="artifact.temp.output." value="${artifacts.temp.dir}/"/>
    <property name="artifact.temp.output.CoffeeShop" value="${artifacts.temp.dir}/CoffeeShop.jar"/>
    <taskdef resource="com/sun/javafx/tools/ant/antlib.xml" uri="javafx:com.sun.javafx.tools.ant" classpath="${jdk.home.22}/lib/ant-javafx.jar"/>
  </target>

  <target name="clean.artifact.coffeeshop" description="clean CoffeeShop artifact output">
    <delete dir="${artifact.output.coffeeshop}"/>
  </target>

  <target name="clean.artifact.coffeeshop2" description="clean CoffeeShop2 artifact output">
    <delete dir="${artifact.output.coffeeshop2}"/>
  </target>

  <target name="artifact.coffeeshop" depends="init.artifacts" description="Build &#39;CoffeeShop&#39; artifact"
          xmlns:fx="javafx:com.sun.javafx.tools.ant"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="javafx:com.sun.javafx.tools.ant ">
    <mkdir dir="${artifact.output.coffeeshop}"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.web.jar"
          tofile="${artifact.temp.output.}/javafx.web.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.base.jar"
          tofile="${artifact.temp.output.}/javafx.base.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.fxml.jar"
          tofile="${artifact.temp.output.}/javafx.fxml.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.media.jar"
          tofile="${artifact.temp.output.}/javafx.media.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.swing.jar"
          tofile="${artifact.temp.output.}/javafx.swing.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.controls.jar"
          tofile="${artifact.temp.output.}/javafx.controls.jar"/>
    <copy file="${basedir}/../Program Files/openjfx-21.0.5_windows-x64_bin-sdk/javafx-sdk-21.0.5/lib/javafx.graphics.jar"
          tofile="${artifact.temp.output.}/javafx.graphics.jar"/>
    <fx:fileset id="all_but_" dir="${artifact.temp.output.}" includes="**/*.jar">
      <exclude name=""/>
    </fx:fileset>
    <fx:fileset id="all_" dir="${artifact.temp.output.}" includes="**/*.jar"/>
    <fx:application id="_id" name="CoffeeShop" mainClass="project.project.coffeeshop.Launcher"></fx:application>
  </target>
